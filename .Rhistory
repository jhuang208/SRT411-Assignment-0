knitr::opts_chunk$set(echo = TRUE)
k=1:10
funct = function(arg1)
{
l = length(arg1)
for(i in 1:l)
{
if (arg1[i] < 5 | arg1[i] > 90)
{
arg1[i] = arg1[i] * 10
} else
{
arg1[i] = arg1[i] * 0.1
}
}
return (arg1)
}
funct(arg1=k)
